{
    "name": "Ãšjallas project",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Application\\": "app/Application",
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.8",
        "cocur/slugify": "^3.1",
        "doctrine/doctrine-bundle": "1.6.9",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^1.0",
        "doctrine/orm": "^2.5",
        "erusev/parsedown": "^1.6",
        "ezyang/htmlpurifier": "^4.9",
        "frcho/crontask": "1.0.2",
        "friendsofsymfony/user-bundle": "~2.0",
        "guzzlehttp/guzzle": "^6.2.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "knplabs/knp-menu-bundle": "^2.0",
        "knplabs/knp-paginator-bundle": "~2.6",
        "knplabs/knp-snappy-bundle": "^1.5.2",
        "martin-georgiev/social-post-bundle": "^2.0",
        "misd/phone-number-bundle": "^1.3",
        "payum/payum-bundle": "^2.3",
        "php-http/guzzle6-adapter": "^1.1",
        "presta/image-bundle": "^1.8",
        "presta/sitemap-bundle": "^1.5",
        "rollerworks/password-strength-bundle": "^2.0",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^5.1.6",
        "sonata-project/admin-bundle": "^3.23",
        "sonata-project/doctrine-extensions": "^1.0",
        "sonata-project/doctrine-orm-admin-bundle": "^3.1",
        "sonata-project/user-bundle": "^4.1",
        "stfalcon/tinymce-bundle": "^2.3",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/cache": "^4.0",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/serializer": "^4.0",
        "symfony/swiftmailer-bundle": "^2.6.4",
        "symfony/symfony": "3.4.*",
        "symfony/webpack-encore-pack": "^1.0",
        "twig/extensions": "^1.5",
        "twig/twig": "^1.0||^2.0",
        "vich/uploader-bundle": "^1.8",
        "white-october/pagerfanta-bundle": "^1.1",
        "whiteoctober/breadcrumbs-bundle": "^1.4",
        "yethee/enum-bundle": "^2.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^4.0",
        "friendsofphp/php-cs-fixer": "^2.7",
        "phpunit/phpunit": "^7",
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "deploy": [
            "php bin/console cache:clear --env=prod",
            "php bin/console asset:install --symlink --relative --env=prod"
        ],
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yaml",
            "env-map": {

            }
        },
        "branch-alias": null
    }

}
